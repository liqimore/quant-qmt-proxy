# 环境变量配置示例
# 复制此文件为 .env 并修改相应配置

# 应用配置
APP_NAME=xtquant-proxy
APP_VERSION=1.0.0
DEBUG=true
HOST=0.0.0.0
PORT=8000

# 日志配置
LOG_LEVEL=INFO
LOG_FILE=logs/app.log

# xtquant配置
XTQUANT_DATA_PATH=./data
XTQUANT_CONFIG_PATH=./xtquant/config

# 安全配置
SECRET_KEY=your-secret-key-here
API_KEY_HEADER=X-API-Key

# 数据库配置(如果需要)
DATABASE_URL=sqlite:///./app.db

# Redis配置(如果需要缓存)
REDIS_URL=redis://localhost:6379/0
